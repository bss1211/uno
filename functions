def my_function():
  print("Hello from a function")

my_function()


def my_function(country = "India"):
  print("I am from " + country)

my_function("Dubai")
my_function("Fiji")
my_function()
my_function("Nepal")

def my_function(food):
  for x in food:
    print(x)

fruits = ["mango", "cherry", "banana"]

my_function(fruits)


def my_function(x):
  return 5 * x

print(my_function(3))
print(my_function(5))
print(my_function(9))


